def get_cricket_players():
    return set(['A1', 'A2', 'A3', 'A4', 'A5', 'B1', 'B2'])

def get_badminton_players():
    return set(['B1', 'B2', 'B3', 'B4', 'B5', 'C1', 'C2'])

def get_football_players():
    return set(['A1', 'A6', 'C1', 'C2', 'C3', 'C4', 'C5'])

# 1. List of students who play both cricket and badminton
cricket_players = get_cricket_players()
badminton_players = get_badminton_players()
both_sports_players = cricket_players.intersection(badminton_players)
print("Students who play both cricket and badminton:", both_sports_players)

# 2. List of students who play either cricket or badminton but not both
either_cricket_or_badminton_players = cricket_players.symmetric_difference(badminton_players)
print("Students who play either cricket or badminton but not both:", either_cricket_or_badminton_players)

# 3. Number of students who play neither cricket nor badminton
all_students = set(['A1', 'A2', 'A3', 'A4', 'A5', 'A6', 'B1', 'B2', 'B3', 'B4', 'B5', 'C1', 'C2', 'C3', 'C4', 'C5'])
neither_cricket_nor_badminton = all_students - (cricket_players.union(badminton_players))
print("Number of students who play neither cricket nor badminton:", len(neither_cricket_nor_badminton))

# 4. Number of students who play cricket and football but not badminton
football_players = get_football_players()
cricket_and_football_but_not_badminton = cricket_players.intersection(football_players) - badminton_players
print("Number of students who play cricket and football but not badminton:"), len(cricket_and_football_but_not_badminton)
